.container {
 background-color:#F8F8F8;
} 
.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.menus{
  display: flex;
  flex-direction: row;
   text-align:center;
  height: 180rpx;
  width: 100%;
  background-color:#fff;
  border-bottom:1rpx solid #E9E9E9;
}
.menu{
  display: flex;
  flex-direction:column;
  align-items: center;
  height: 100%;
  width: 50%;
  padding-top:25rpx;
  box-sizing:border-box;
}
.picture,.picture2{
  width:90rpx;
  height:90rpx;
  border-radius:50%;
}
#yellow{
    background-color:#FAD041;
}
#green{
  background-color:#9BDF55;
}
.menu image{
  margin:15rpx;
  width:60rpx;
  height:60rpx;
}
 .word,newtext{
    font-size:28rpx;
}
.new{
  display: flex;
  flex-direction:column;
  align-items: center;
  height:550rpx;
  width:100%;
  margin-top:20rpx;
  background-color:#fff;
}
.words{
  display: flex;
  flex-direction:row;
  align-items: center;
  font-size:32rpx;
  height:80rpx;
  line-height:80rpx;
}
.newpicture,.newtext{
  width:60rpx;
  height:60rpx;
  line-height:60px;
}
.newtext{
  width:60rpx;
  height:60rpx;
  line-height:60px;
}
.newreport{
  width:100%;
  height:150rpx;
}
.foods{
  display: flex;
  flex-direction: row; 
  width:100%;
  height:120px;
  margin-top:40rpx;
}
.food{
  display: flex;
  flex-direction:column;
  align-items: center;
  width:31%;
  margin-left:1.5%;
}
.foodpic{
  width:100%;
}
.foodprice{
    font-size:28rpx;
    color:#FF0036;
}
.more{
  height:100rpx;
  line-height:100rpx;
  width:100%;
  font-size:28rpx;
  background-color:#fff;
  margin-top:20rpx;
  text-align:center;
}
.myname{
  width:100%;
  height:100rpx;
  text-align:center;
  font-size:20rpx;
  line-height:100rpx;
}